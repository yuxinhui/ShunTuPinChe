<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jinfukeji.shuntupinche">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.READ_SMS"/>
    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW"/>

    <application
        android:name=".ShunTuApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/NoActionBar">
        <activity android:name=".FirstStartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.LoginActivity"/>
        <activity android:name=".activity.RegistActivity"/>
        <activity android:name=".activity.BannerActivity"/>
        <activity android:name=".activity.RegistSuccessActivity"/>
        <activity android:name=".activity.owenr.OwenrIndexActivity"/>
        <activity android:name=".activity.owenr.OwenrHistoryRecordActivity"/>
        <activity android:name=".activity.owenr.OwenrVerifyActivity"/>
        <activity android:name=".IndexActivity"/>
        <activity android:name=".weather.MyTianQiActivity"/>
        <activity android:name=".yindaotu.GuideActivity"/>
        <activity android:name=".SplashActivity"/>
    </application>

</manifest>